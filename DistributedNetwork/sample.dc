import DGameObject
import AIDGameObject/AI

struct gameDataModel {
    string value_a;
    uint8 value_b;
    int8 value_c/100;
}

dclass DGameObject: DistributedObject {
    send_game_data(gameDataModel data) broadcast;
};

dclass AIDGameObject: DistributedObject {
    message_roundtrip_to_ai(gameDataModel data) p2p;
    message_roundtrip_to_client(gameDataModel data) p2p;
}